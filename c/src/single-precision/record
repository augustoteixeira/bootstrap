trap 'end=$(date +%s); echo "# Elapsed Time: $(($end-$start)) seconds" >> ${1}.csv' INT

if [ $# -eq 0 ]
  then
    echo "No arguments supplied"
    exit 1
fi


if g++ -o a.out ${1}.cpp -lm; then
  echo "Compilation succeeded"
else
  echo "Compilation failed"
  exit 1
fi

start=$(date +%s)
./a.out | tee ${1}.csv
